{
  "name": "kratos-launcher",
  "productName": "Kratos",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/electron/index.js",
  "scripts": {
    "build": "node ./scripts/cli.js build all",
    "clean": "node ./scripts/cli.js clean",
    "test-unit": "cross-env NODE_ENV=test mocha",
    "coverage": "cross-env NODE_ENV=test nyc npm run test",
    "start": "cross-env NODE_ENV=production electron-forge start",
    "dev": "concurrently -c \"auto\" \"node ./scripts/watch-render.js\" \"wait-on file:./dist/render && node ./scripts/cli.js build electron && electron .\" ",
    "test-e2e": "npm run build && cross-env DEBUG=pw:browser playwright test",
    "test": "concurrently -c \"auto\" \"npm:test-*\" --kill-others-on-fail -g --timings",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "keywords": [
    "minecraft launcher",
    "kratos minecraft launcher",
    "kratos"
  ],
  "author": "PlayerNguyen",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@electron-forge/cli": "^6.0.4",
    "@electron-forge/maker-deb": "^6.0.4",
    "@electron-forge/maker-rpm": "^6.0.4",
    "@electron-forge/maker-squirrel": "^6.0.4",
    "@electron-forge/maker-zip": "^6.0.4",
    "@parcel/config-default": "^2.8.3",
    "@parcel/core": "^2.8.3",
    "@playwright/test": "^1.29.2",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.31",
    "@types/chai": "^4.3.4",
    "@types/fs-extra": "^11.0.1",
    "@types/mocha": "^10.0.1",
