    "@types/react-dom": "^18.0.10",
    "@types/uuid": "^9.0.0",
    "chai": "^4.3.7",
    "concurrently": "^7.6.0",
    "daisyui": "^2.49.0",
    "dotenv": "^16.0.3",
    "electron": "^22.2.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "playwright": "^1.29.2",
    "postcss": "^8.4.21",
